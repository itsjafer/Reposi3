<package>
	<id>com.itsjafer.stopshortcutsnotifications</id>
	<name>Stop Shortcuts Notifications</name>
	<version>1.1.0</version>
	<compatibility>
		<firmware>
			<maxiOS>13.3</maxiOS>
			<otherVersions>unconfirmed</otherVersions>
			<!--
			for otherVersions, you can put either unsupported or unconfirmed
			-->
		</firmware>
	</compatibility>
	<dependencies></dependencies>
	<descriptionlist>
		<description>This tweak prevents the shortcuts app from sending notifications.</description>
		<description>In particular, it speeds up the process for running shortcuts automations. Take a look at the screenshots to see how much faster it runs.</description>
	</descriptionlist>
	<screenshots>
		<screenshot>
			<description>Disabled gif</description>
			<image>before.gif</image>
		</screenshot>
		<screenshot>
			<description>Enabled gif</description>
			<image>after.gif</image>
		</screenshot>
	</screenshots>
	<changelog>
		<change>Added option to only disable automation notifications.</change>
	</changelog>
	<links></links>
</package>